<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>vCat</display-name>

	<servlet>
		<servlet-name>vcat-servlet</servlet-name>
		<servlet-class>vcat.toollabs.webapp.ToollabsVCatServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>catgraph-servlet</servlet-name>
		<servlet-class>vcat.toollabs.webapp.ToollabsCatgraphServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>catgraph-converter-servlet</servlet-name>
		<servlet-class>vcat.toollabs.webapp.ToollabsCatgraphConverterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>vcat-servlet</servlet-name>
		<url-pattern>render</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>catgraph-servlet</servlet-name>
		<url-pattern>catgraph-redirect</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>catgraph-converter-servlet</servlet-name>
		<url-pattern>catgraph-convert</url-pattern>
	</servlet-mapping>

</web-app>
